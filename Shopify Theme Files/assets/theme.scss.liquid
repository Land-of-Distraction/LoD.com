	}

	.utility-menu{
		@extend .copy7;
		ul{
			li{
				a{
					@extend .copy7;
					position: relative;
				}
			}
		}
	}//.utility-menu

	.tab-toggle{
		display: block;
		width: 100%;
		position: relative;
		float: none;
		clear: both;
		padding: 15px 20px 15px 0;
		cursor: pointer;
		@extend .copy2;
		// font-family: $heavyFont;
		@include icon-fonts-before($icon-lod-arrow-right, inherit, 18px);
		&:before{
			@include transition(all $slow);
			@include transform(rotate(90deg));
			position:absolute;
			right: 0;
			top:15px;
		}

		& + div{
			margin-left: 0;
			max-height: 1px;
			@include ruler(bottom,$bodyColor4);
			overflow: hidden;
			color: transparent;
			line-height: 1.4;
			@include clear-box();
			@include transition(all $slower);
			a{
				color: inherit;
			}
		}
		& + div + br {//Shopify add br after the tab toggles
			display: none;
		}

		&.active{
			&:before{
				@include transform(rotate(-90deg));
			}
			& + div{
				margin-bottom: 15px;
				color: inherit;
				overflow:visible;
				max-height: 200vh;
				@include clear-box();
				a{
					color: inherit;
				}
			}
		}
