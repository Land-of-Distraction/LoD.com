@include query-tablet-mobile {
	$spacingTop : $headerDesktopHeight + $promoBannerHeight;
	$collectionNavWidth: 100vw;
	$negCollectionNavWidth: -#{$collectionNavWidth};

	.template-collection{
		padding-top: $spacingTop;
		header.site-header{
			width: 100vw;
			left: 0;
		}
		#MainContent,
		header.site-header,
		footer.site-footer{
			width: 100vw;
			@include transition(margin-left $fast ease-in-out);
		}
	}

	html{
		// overflow-x: hidden;
	}

	.collection-nav {
		.collection-nav-toggle{
			margin-bottom: $desktopMargin;
		}
		.collection-nav-wrapper{
			padding-left:0;
			padding-right:0;
			width: $collectionNavWidth;
			z-index: 100;
			left: $negCollectionNavWidth;
			padding-top: $spacingTop;
		}
		.collection-nav-block{
			opacity: 0;
			padding:0 $mobileMargin;
			@include transition(opacity $fast ease-in-out);
			@include transition-delay($fast);
			.nav-content{
				margin-bottom: $mobileMargin/2;
				.filter{
					.filter-swatches{
						margin-bottom: $mobileMargin/2;
					}
				}
			}
		}
	}//.collection-nav

	.collection-nav-opened{//on body
		overflow-x: hidden;
		//1. Move content elements to the right
		#MainContent,
		header.site-header,
		footer.site-footer{
			margin-left: $collectionNavWidth;
			width: 0;
			padding:0;
			overflow: hidden;
		}

		//2. Keep the nav on the left edge while the other content is on the side
		.collection-nav{
			.collection-nav-wrapper{
				left: 0;
			}
			.collection-nav-block{
				opacity: 1;
			}
		}
	}

	article.item{
		.quick-buy-container{
			display: none;
		}
		.product-info-wrapper{
			.price-box{
				span{
					display: inline-block;
					padding: 0 5px;
				}
				.product-compare-price{
					display: inline-block;
				}
			}
		}
	}

	.collection-products {
		padding-top: 0;
		justify-content: space-between;
		.gutter-sizer{
			width: $mobileMargin;
		}
		article.item{
			padding-bottom: $mobileMargin;
			@include transition-delay($fast);
			@include transform(translateY(0%));
			.quick-buy-container{
				display: none;
			}
			&.promo-item{
				display: flex;
				align-items: center;
				justify-content: center;
				padding-bottom: 80px;
				img{
					max-width: 85%;
					margin: 0 auto;
				}
			}
		}
	}
};//query-tablet-mobile

