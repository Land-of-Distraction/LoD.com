	.video-wrapper {

		iframe, video {
			opacity: 0;
		}

		iframe {
			z-index: 3;
		}

		video {
			z-index: 0;
		}

		div.video-box {
			&[data-video-type="vimeo"] {
				z-index: 3;
			}
		}

		.video-ctl {
			width: 100%;
			height: 100%;
			opacity: 1;
			top: 0;
			left: 0;
			&:before {
				@include vcenter-fallback;
			}
		}
	}

};

