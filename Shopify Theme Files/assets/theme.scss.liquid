$loaderBorderSize: 9px;
$loaderInnerBorderSize: 3px;

.onemodal {
	height: 100%;
	left: 0;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 1000;

	.box-content {
		left: 50%;
		top: 50%;
		position: absolute;
		z-index: 1001;
		box-shadow: 0px 26px 102px -27px rgba(0,0,0,0.21);
		background: $boxContentBg;
		overflow: hidden;
		@include opacity(0);

		.box-inner {
			display: inline-block;
			@include opacity(0);

			.loading {
			}

			.loadingtext {
				position: absolute;
				text-align: center;
				//top: 50%;
				top: 75%;
				width: 100%;
				margin-top: -6px;
				color: black;
				text-transform: uppercase;
			}

			.alert-message {
				// Default
				display: block;
				padding: 15px;
				//max-width: 974px;
				min-width: 300px;

				p { white-space: pre-wrap; display: block; }

				&.success {}
