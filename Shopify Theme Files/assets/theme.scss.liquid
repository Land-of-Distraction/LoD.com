			.images-wrapper{
				display: flex;
				overflow: auto;
				flex-wrap: nowrap;
				vertical-align: top;
				justify-content: flex-start;
				flex-direction: row;
				a{
					display: block;
					position: relative;
					width: 100vw;
					flex: 0 0 auto;
				}
			}
		}//.product-media

		.product-info{
			position: relative !important;
			width: 100% !important;
			// z-index: 10 !important;
			.product-info-wrapper{
				padding: $mobileMargin $mobileMargin $mobileMargin*2;
			}
			.add-to-cart-form{
				margin-top: $desktopMargin;
			}
			.price-box{
				span{
					display: inline-block;
				}
			}
			.product-select-wrapper{
				display: flex;
				justify-content: space-between;
			}
			.selector-wrapper{
				width: 47%;
				border-bottom: 1px solid;
				select{
					//to avoid iOs stupid zoom  //12px/16px
					font-size: 16px;
					max-width: none;
					border-bottom: none;
					$selectFieldWidth: 1 + (0.75 * 0.47);
					width: calc(100% * #{$selectFieldWidth});
					@include transform(scale(0.75) translateX(0%));
					line-height: $desktopMargin;
					transform-origin: 0 50%;
				}
			}
		}//.product-info

		.inner-wrapper{
			margin-bottom: $mobileMargin*2;
		}//.inner-wrapper

		.floating-content{
			position: relative;
			.product-content-nav{
				position: absolute;
				top: -#{$mobileMargin*2};
				width: calc(100% - #{$mobileMargin*2});
				left: $mobileMargin;
			}
			.product-content-wrapper{
				position: fixed;
				bottom: 0;
				left: 0;
				right: 0;
				display: flex;
				flex-direction: column;
				z-index: 10;
				.pdp-utility{
					// position: fixed;
					// bottom: 0;
					// left: 0;
					// right: 0;
					z-index: 0;
					opacity: 0;
					visibility: hidden;
					padding: 0 $desktopMargin;
					max-height: 0;
					overflow: hidden;
					// @include transform(translateY(100%));
					@include transition(all $fast ease-in-out);
					&.open{
						opacity: 1;
						visibility: visible;
						max-height: 100vh;
						padding: $mobileMargin*3 $desktopMargin;
						@include transform(translateY(0));
					}
					&.size-chart{
						.description-wrapper{
							padding-bottom: $mobileMargin;
							.help-link{
								bottom:0;
								right: 0;
							}
						}
					}
				}
			}
		}//.floating-content

		.one-social .share-list a{
			font-size: 16px;
			line-height: $mobileMargin;
			text-align: center;
			min-width: $mobileMargin;
			margin-left: $mobileMargin/2;
			margin-right: $mobileMargin/2;
			&.copy-link{
				font-size: 12px;
