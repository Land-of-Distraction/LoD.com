::-webkit-selection{background: $bodyColorOpacity2;color: $white;}

body,
input,
textarea,
button,
select {
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
  outline: none;
  padding: 0;
  margin: 0;
}

ul, li {
	list-style: none;
	padding: 0;
	margin: 0;
}

a {
	color: $color-body-font;
	text-decoration: none;
}
a:focus {
  color: inherit;
}

img {
  max-width: 100%;
}

/*================ Form element helpers ================*/
form {
  margin: 0;
}


button,
input[type="submit"],
label[for] {
  cursor: pointer;
}

optgroup {

}

// Force option color (affects IE only)
option {
  color: $black;
  background-color: $color-body;
}

// hide outline on focus for elements which are given focus by JS
[tabindex='-1']:focus {
  outline: none;
}

/*============================================================================
  Fast Tap
  enables no-delay taps (FastClick-esque) on supporting browsers
==============================================================================*/
a,
button,
[role="button"],
input,
label,
select,
textarea {
  touch-action: manipulation;
}

/*================ Links & Buttons ================*/
.btn {
	@include prefix('user-select', 'none', moz ms webkit spec);
	@include prefix(appearance, none, webkit moz spec);
	display: inline-block;
	width: auto;
	text-decoration: none;
	text-align: center;
	vertical-align: middle;
	white-space: nowrap;
	border: 0;
}

//
//  Tablet and Mobile
//  _____________________________________________
@include query-tablet-mobile {
	// Prevent zoom on touch devices in active inputs
	input,
	textarea,
	select {
	  font-size: 16px;
	}
};

/*================ Helper Classes ================*/
.clearfix {
  @include clearfix();
}

.visually-hidden {
  @include visually-hidden();
}

.js-focus-hidden:focus {
  outline: none;
}

.label-hidden {
  @include visually-hidden();

  // No placeholders, so force show labels
  .no-placeholder & {
	@include visually-shown();
  }
}

.visually-shown {
  @include visually-shown();
}

// Only show when JS is not supported
.no-js:not(html) {
  display: none;

  .no-js & {
	display: block;
  }
}

// Only show when JS is supported
.js {
  .no-js & {
	display: none;
  }
}

// Only show when browser cookies are not supported
.supports-no-cookies:not(html) {
  display: none;

  html.supports-no-cookies & {
	display: block;
  }
}

// Only show when browser cookies are supported
.supports-cookies {
  html.supports-no-cookies & {
	display: none;
  }
}

.hide {
	display: none;
}

/*================ Rich Text Editor ================*/
.rte {
  img {
	height: auto;
  }

  table {
	table-layout: fixed;
  }

  // Match the styles from RTE nested lists
  ul {
	list-style: disc outside;

	ul {
	  list-style: circle outside;

	  ul {
		list-style: square outside;
	  }
	}
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
	margin-left: 0;
	list-style-position: inside;
  }
}

// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

/*============================================================================
  Form scaffolding
	- Selectors setup for you to style form elements how you want
	- Focus, error, and disabled states are set to be extended
==============================================================================*/
$input-line-height: 33px;
$input-line-height-mobile: 44px;

///Remove chrome autofill bg *not firefox*
input:-webkit-autofill,
textarea:-webkit-autofill,
select:-webkit-autofill,
:-webkit-autofill:hover,
:-webkit-autofill:focus,
:-webkit-autofill:active {
	background: rgba(0,0,0,0.10);
	-webkit-text-fill-color: $white;
	transition: background-color 5000s ease-in-out 0s;
	-webkit-transition: background-color 5000s ease-in-out 0s;
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
	color: $white !important;
	background-color: rgba(0,0,0,0.05) !important;

	// make sure to autofill to black text on updated white bg checkout pages
	.template-checkout & {
	  -webkit-text-fill-color: $black;
	  color: $black !important;
	  -webkit-animation-name: autofill-black;
	  animation-name: autofill-black;
	  animation-fill-mode: both;
	}
}

@-webkit-keyframes autofill {
	to {
		color: $white;
		background: rgba(0,0,0,0.05);
	}
}

@-webkit-keyframes autofill-black {
	to {
		color: $black;
		background: rgba(0,0,0,0.05);
	}
}

input,
textarea,
button,
select{
  font-family: $bodyFont;
  font-size: $baseFontSize;
  font-weight: $baseFontWeight;
  // color: $bodyColor;
  text-shadow: none;
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust:100%;
  line-height: $baseLineHeight;
  @include clean-input-appearance;
}

input,
textarea,
select {
	border: 0;
	border-bottom: 1px solid $color-border;
	border-radius: 0;
	max-width: 100%;
	padding: 0 10px;
	margin: 0;
	line-height: $input-line-height;
	&[disabled] {
		cursor: default;
		background-color: $color-disabled;
		border-color: $color-disabled-border;
	}
}
button{
  background: transparent;
  border: none;
}
textarea {
	min-height: 100px;
}

div.qty {
	& > * {
		visibility: hidden;
		opacity: 0;
		@include transition(all $slow);
	}
	& > .js-qty {
		visibility: visible;
		opacity: 1;
	}
}
.js-qty, .ajaxcart__qty {
	$qty-size: 30px;
	margin-bottom: 10px;
	.qty_caption{
		font-size: 12px;
	}
	button {
		display: none;
		&.ajaxcart__qty--minus,
		&.js-qty__adjust--minus {
			@include icon-btn($icon-chevron-left, $black, $qty-size);
		}
		&.ajaxcart__qty--plus,
		&.js-qty__adjust--plus {
			@include icon-btn($icon-chevron-right, $black, $qty-size);
		}
	}
	input {
		text-align: center;
		width: $qty-size;
		line-height: 25px;
		height: 25px;
		padding: 0;
		margin: 0 10px;
		background: transparent;
	}
	& > * {
		display: inline-block;
		vertical-align: middle;
	}
}

/*================ Custom select style ================*/
select {
	@include prefix(appearance, none, webkit moz spec);
	width: 100%;
	padding-right: 20px;
	text-overflow: '';
	position: relative;
	cursor: pointer;
	background: transparent;
	/*================ Hide the svg arrow in IE9 ================*/
	.ie9 & {
		background-image: none;
	}
}
select::-ms-expand {
	display: none;
}

/*================ Error styles ================*/
input,
select,
textarea {
	&.input-error {
		border-color: $color-error;
		color: $color-error;
	}
}

//
//  Tablet and Mobile
//  _____________________________________________
@include query-tablet-mobile {
	input, select {
		line-height: $input-line-height-mobile;
	}
}

//
//  Common
//  _____________________________________________
html, body {
	// Sticky Footer
	height: 100%;
	// display: flex;
	// flex-direction: column;
	// overflow-x: hidden;
	#MainContent {
		flex: 1 0 auto;
	}
	&.zooming {
		overflow: hidden;
	}
}

body{
	@extend .animated, .fadeIn;
	@include animation-delay($fast);
	@include animation-duration(0.6s);
	@include transition(padding-top $fast);
}

body {
	&:before {
		// Overlay
		content: "";
		position: fixed;
		width: 100%;
		height: 100vh;
		background: rgba(0,0,0,0);
		top: 0;
		left: 0;
		opacity: 0;
		visibility: hidden;
		z-index: 101;
		@include transition(all $slow);
	}
	&.show-overlay {//, &.search-opt
		&:before {
			// Overlay
			opacity: 1;
			visibility: visible;
		}

	}
	&.drawer-opt {
		overflow: hidden;
		&:before {
			z-index: 101;
		}
	}
}

.pick_currency {
	position: absolute;
	display: none !important;
}

.drawer {
	$drawerWidth : 400px;
	width: 75%;
	max-width: $drawerWidth;
	height: 100%;
	background: $black;
	color: $white;
	padding: $mobileMargin $desktopMargin;
	overflow: hidden;
	position: fixed;
	z-index: 1002;
	top: 0;
	opacity: 1;
	visibility: hidden;
	@include transition(all $fast ease-in-out);

	&.drawer-right {
		right: 0;
		@include transform(translateX(120%));
		.drawer-close {
			right: 0;
			top: 0;
		}
	}

	&.drawer-left {
		left: 0;
		@include transform(translateX(-100%));
		.drawer-close {
			right: 0;
			top: 0;
		}
	}

	&.opened {
		opacity: 1;
		visibility: visible;
		@include transform(translateX(0));
	}

	.drawer-close {
		position: absolute;
		button {
			display: block;
			line-height: 1;
			@include icon-btn($icon-close, inherit, 18px);
		}
	}
}//.drawer

#MobileNav {

	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
	flex-wrap: wrap;
	$mobileUtilityHeight: 120px;
	height: auto;
	min-height: 100%;
	width: 100%;
	max-width: 100%;
	padding: $headerMobileHeight 0 $mobileUtilityHeight;

	.drawer-close{
		button{
			padding: $mobileMargin;
		}
	}

	nav{
		width: 100%;
		opacity: 0;
		position: relative;
		@include transform(translateY(40%));
		@include transition(all $slow ease-in-out);
	}

	.site-nav {
		.level-1, .level-2  {
			& > .nav-link {
				@extend .nav1;
				position: relative;
				text-align: center;
				color:inherit;
				line-height: $mobileMargin*2;
				display: block;
			}
		}
		.level-1 {
			overflow-y: hidden;
			& > .nav-link{
				@include transition(all $slow);
				@include transform(translateX(0%));
				color: $white;
			}

			&.opened {
				overflow-y: hidden;
				.children {
					opacity: 1;
					visibility: visible;
					max-height: 300vh;
					@include transition-delay($fast);
					@include transform(translateY(0%));
					& > li{
						margin:0 $mobileMargin*2;
					}
				}

				&.mobile-newsletter{
					.newsletter-toggle {
						opacity: 1;
					}
				}
			}
		}
	}//.site-nav

	.children,
	.mobile-block-children {
		visibility: hidden;
		opacity: 0;
		overflow: hidden;
		height: auto;
		max-height:0;
		width: 100%;
		z-index: 1;
		@include transition(all $slow ease-in-out);
		@include transform(translateY(40%));
	}

	.mobile-newsletter{
		.children{
			li{
				margin: 0 40px;
			}
		}
		.newsletter-form{
			opacity: 0;
			overflow: hidden;
			position: relative;
			@include transition(all $fast ease-in-out);
			.input-group{
				input{
					font-size: 16px;
					&::placeholder{
						font-size: 14px;
					}
				}
			}
		}
		&.opened{
			.newsletter-form{
				opacity: 1;
			}
		}
	}

	.mobile-search-toggle {
		@extend .nav1;
		color: inherit;
		display: block;
		margin-bottom: $desktopMargin2*2;
		position: fixed;
		bottom: 0;
		left: 20%;
		right: 20%;
		padding: 11px 0;
		text-align: center;
		border: none;
		@extend .animated, .fadeIn;
		@include animation-delay($quick);
		@include animation-duration($slow);
	}

	.block-search {
		position: relative;

		.block-content {
			visibility: hidden;
			opacity: 0;
			overflow: hidden;
			position: fixed;
			z-index: -1;
			display: flex;
			width: 100%;
			justify-content: center;
			align-items: center;
			padding: 0 $mobileMargin;
			left: 0;
			right: 0;
			top: $mobileMargin*2;
			bottom: $mobileMargin*5;
			background: $white !important;

			.search-close{
				display: none;
			}
			.search-form{
				opacity: 0;
				@include transition(all $fast ease-in-out);
				@include transform(translateY(40%));
			}
			form {
				$search-input-height: 50px;
				$search-btn-width: 50px;
				display: block;
				width: 100%;
				position: relative;
				color: $black;
				max-width: 640px;
				overflow:visible;
				@include ruler(bottom,$white);
				padding-bottom:12px;
				text-align: center;
				input {
					background: transparent;
					padding: 0;
					text-align: center;
					border: 0;
					color: $black !important;
					text-transform: uppercase;
					line-height: 1;
					@extend .heading2;
					//Below code to avoid iOs zoom on input
					font-size: 16px;
					letter-spacing: 0.4px;
					padding: 1px 0;
					@include transform(scale(1.6));
					width: calc(100% / 1.6);
					padding-right: 10px;
					border-bottom:none;
					z-index: 10;

					&::placeholder{
						color: $black;
						text-transform: uppercase;
						line-height: 1;
						font-size: 16px;
					}
				}
				button {
					@include icon-btn($icon-lod-arrow-right, $black, 20px);
					position: absolute;
					right: 0;
					top: -15px;
					text-align: right;
					width: $search-btn-width;
					height: $search-input-height;
				}
			}
		}
		&.active {
			.block-content {
				visibility: visible;
				opacity: 1;
				z-index: 1;
			}
			.search-form{
				opacity: 1;
				@include transition-delay($fast);
				@include transform(translateY(0%));
			}
		}
	}//.block-search

	&.child-menu-opened {
		.site-nav .level-1 > .nav-link{
			opacity: 0.4;
		}
	}

	&.opened {
		nav{
			opacity: 1;
			@include transition-delay($quick);
			@include transform(translateY(0%));
		}
	}
	&.search-open{
		nav{
			opacity: 0;
			@include transition-delay($quick);
			@include transform(translateY(40%));
		}
	}

	.header-utilities{
		.block-search{

		}
		.block-account{
			display: none;
		}
	}

}//#MobileNav

