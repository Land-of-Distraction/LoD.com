	margin: 50px auto;
	@include transition(all $slow);

	&.full-bleed {
		max-width: 100%;
	}

	.bleeding-ctl,
	.frame-ctl {
		cursor: pointer;
		position: fixed;
		top: 50%;
		left: 0;
		background: rgba(200,200,200,0.3);
		border: 1px solid transparent;
		border-radius: 50%;
		width: 100px;
		height: 100px;
		z-index: 10;
		@include transition(all $slow);
		@include transform(translate(-70%,-50%));

		&:hover {
			width: 150px;
			height: 150px;
			background: rgba(100,100,100,0.9);
			@include transform(translate(-50%,-50%));
		}

		&.frame-ctl {
			left: auto;
			right: 0;
			@include transform(translate(70%,-50%));

			&:hover {
				@include transform(translate(50%,-50%));
			}
		}
	}

	.grid-system {
		.grid-row {
			@include clear-box;
			background-color: rgba(212,12,118,0.3);
			position: relative;

			&:before {
				content: attr(title);
				display: block;
				position: absolute;
				font-size: 1.16666667rem;
				line-height: 72px;
				left: 1%;
			}
		}

		.grid-col {
			height: 72px;
			float: left;
			position: relative;

			&:after {
				content: "";
				position: absolute;
				display: block;
				width: 100%;
				height: 100%;
				top: 0;
				left: 0;
				opacity: 0;
				border: 1px solid yellow;
				box-sizing: border-box;
				@include transition(all $slow);
			}

			&.show-frame {
				&:after {
					opacity: 1;
				}
			}
		}

		&.show-frame {
			.grid-col::after {
				opacity: 1;
			}
		}

		.grid-col-1 {
			background-color: rgba(74,144,226,0.3);
		}

		.grid-col-with-margin-1 {
			background-color: rgba(74,144,226,0.3);
		}

		.grid-col-2, .grid-col-with-margin-2 {
			background-color: #ED258F;
		}

		.grid-col-3, .grid-col-with-margin-3 {
			background-color: #E15B50;
		}

		.grid-col-4, .grid-col-with-margin-4 {
			background-color: #529E76;
		}

		.grid-col-5, .grid-col-with-margin-5 {
			background-color: #2F013E;
		}

		.grid-col-6, .grid-col-with-margin-6 {
			background-color: #E15B50;

			&:first-child {
				background-color: #69027E;
			}
		}

		.grid-col-7, .grid-col-with-margin-7 {
			background-color: #4A90E2;
		}

		.grid-col-8, .grid-col-with-margin-8 {
			background-color: #4A4A4A;
		}

		.grid-col-9, .grid-col-with-margin-9 {
			background-color: #B8E986;
		}

		.grid-col-10, .grid-col-with-margin-10 {
			background-color: #F5A623;
		}

		.grid-col-11, .grid-col-with-margin-11 {
			background-color: #50E3C2;
		}

		.grid-col-12, .grid-col-with-margin-12 {
