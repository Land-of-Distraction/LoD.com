	.wrap{
		background: $white;
		color: $black;
		padding:0;
		// overflow:hidden;
		@extend .animated, .fadeIn;
		@include animation-delay($slower);
		@include animation-duration($slow);
	}
	.main{
		padding: $mobileMargin $mobileMargin $desktopMargin2;
	}
	.sidebar{
		padding:0 $mobileMargin;
		color: $black;
		background: $white;
		&:after{
			display: none;
		}
	}

	.section__header{
		a{
			border-bottom: 1px solid;
			line-height: $mobileMargin;
			display: inline-block;
		}

		&.os-header{
			margin-bottom: 0;
			svg{
				position: relative;
				right: inherit;
				top: inherit;
				transform: none;
				stroke: $white;
				@extend .animated, .zoomIn;
				@include animation-delay(1.2s);
				@include animation-duration($slow);
				display: none;
			}
			@include icon-fonts-before($icon-tickbox-check, $bodyColor3, $mobileMargin);
			&:before{
				margin-right:$mobileMargin;
				@extend .animated, .bounceIn;
				@include animation-delay(1.2s);
				@include animation-duration($fast);
			}
			.os-header__heading{
				display: inline-block;
			}
		}
	}//.section__header

	.main h2,
	.section__title{
		@extend .copy2;
		color: $black;
	}

	.main h3{
		font-family: inherit;
	}

	.step{
		&[data-step="thank-you"]{
			.section__content{
				.fieldset.fieldset--inline{
					flex-wrap: wrap;
					margin: 0;
				}
				.btn{
					margin-right: 0;
					margin-left: 0;
				}
			}
		}
	}

	.redacted-line {
		background: $darkGrey;
		border-radius:2px;
	}

	.section__text{
		letter-spacing: 0.5px;
		color: $lightGrey2;
	}
	.section{
		overflow: hidden;
		&.section--shipping-address{
			@include ruler(top,$white);
			padding-top: 25px;
			margin-top: 15px;
		}
		&.section--payment-method{
			overflow:visible;
		}
	}

	.policy-list__item{
		a{
			font-size: 12px;
		}
	}

	.main__footer{
		border-top: none;
		padding:0;
		font-size: 12px;
		.copyright-text{
			display: none;
		}
		.copyright{
			color: $lightGrey2;
				font-size: 12px;
			span{
				color: inherit;
				font-size: 12px;
			}
		}
	}//.main__footer

	.step__footer{
		text-align: center;
		display: flex;
	}

	.step__footer__previous-link{
		flex:1;
		line-height: 34px;
		.step__footer__previous-link-content{
			@extend .checkoutCopy;
			letter-spacing: 0.5px;
			color: $lightGrey2;
			font-size: 13px;
			@include icon-fonts-before($icon-chevron-left, inherit, inherit);
			&:before{
				margin-right: 10px;
				display: inline-block;
			}
		}
		svg{ display: none; }
	}

	.logged-in-customer-information__avatar{
		background:none !important;
	}

	// Alt payment options
	.alt-payment-list{
		display: inline-block;
		vertical-align: top;
		text-align: center;
		width: 100%;
	}

	.alt-payment-list__item{
		display: inline-block;
		vertical-align: top;
		text-align: center;
	}

	.alt-payment-list__item__link{
		height:32px;
		padding:0;
		cursor: pointer;
	}

	//Custom Alt Payment Logos
	.alt-payment-list__item--paypal,
	.alt-payment-list__item--amazon,
	.alt-payment-list__item--apple-pay{
		background-position: center center;
		background-repeat:no-repeat;
		background-size: 68px;
		background-color: black;
		transition: background-color $fast;
		border: 1px solid $lightGrey2;
		border-radius:0;
		text-indent: -9999px;
		padding:0 $desktopMargin/2;
		@include transition(all $fast);
		img{
			//visibility: hidden;
			opacity: 0;
		}

		div{
			background-image: none;
		}

		&:hover{
			border-color: $white;
		}
	}

	.alt-payment-list__item--paypal{
		background-image: url(checkout-paypal.png);
		background-position: center 7px;
	}
	.alt-payment-list__item--amazon{
		background-image: url(checkout-amazon-pay.png);
		background-position: center 11px;
	}
	.alt-payment-list__item--apple-pay{
		background-image: url(checkout-apple-pay.png);
		background-size: 45px;
		.alt-payment-list__item__logo,
		span{
			background-image:none;
		}
	}

	// Form Config
	.field__message--error {
		margin: 0.75em 0.45em 0.25em;
	}

	.field__label {
		font-size: 12px;
		padding:0 $mobileMargin/2;
		line-height: $mobileMargin;
		margin: 0;
	}

	.field__input,
	.boxshadow .main .field__input,
	.boxshadow .sidebar .field__input {
		// @extend .checkoutCopy;
		letter-spacing: 0.5px;
		background: transparent;
		color: $black;
		box-shadow: none;
		border-radius:0;
		height: 51px;
		padding:$mobileMargin $mobileMargin/2 0;
		line-height: $desktopMargin;
	margin-bottom: $mobileMargin/2;
	border: none;
		border-bottom: 1px solid $bodyColor4;
	}

	.input-checkbox {
		border-radius: 0;
	-webkit-box-shadow: 0 0 0 0 transparent inset;
		box-shadow: 0 0 0 0 transparent inset;

	@include ruler("", #ccc);
	@include icon-fonts-after($icon-check, $white, 16px);

	&:after {
	  background:none;
	  line-height: 1;
	  opacity: 0;
	  background:none;
	  font-size: 16px;
	  color: $black;
	  width: auto;
	  height: auto;
	  margin: 0;
	  left: 0;
	  top: 0;
	  @include transition(all $fast);
	  @include transform(scale(0.2));
	}

		&:checked {
			// -webkit-box-shadow: 0 0 0 10px $lightGrey2 inset;
			// box-shadow: 0 0 0 10px $lightGrey2 inset;
			overflow: hidden;
			border-color: $black;

	  &:after {
		opacity: 1;
				@include transform(scale(0.9));
	  }
		}

		&:hover,
		&:focus {
			border-color: $black;
		}
	}

	.input-radio {
		&:checked {
			-webkit-box-shadow: 0 0 0 10px $lightGrey2 inset;
			box-shadow: 0 0 0 1px $lightGrey2 inset;
		}

	&:after {
			width: 8px;
			height: 8px;
			margin-left: -4px;
			margin-top: -4px;
		}
	}

	.checkbox__label {
		// @extend .checkoutCopy;
		letter-spacing: 0.5px;
		color: $lightGrey2;
	}

	.btn {
		@include cta;
		@extend .animated, .fadeIn;
		@include animation-delay($slow);
		@include animation-duration($fast);

	padding:0;

	.btn__content {
			@extend .copy2;
		}

		&.step__footer__continue-btn {
			padding:0 $desktopMargin2;
		}

		&.remember-me-button {
			width: auto;
			border-color: $bodyColor4;
			color: $lightGrey2;
			border-radius: 4px;
			padding: 0 10px;

	  &:hover{
				color: $black;
			}

	  svg {
				width:$desktopMargin;
			}
		}
	}

	.btn__spinner {
		transform: translateY(-50%);
		margin-top: 0;
		height: 100%;
		line-height: 1;
		top: 0;
	}

	.boxshadow .btn:focus,
	.boxshadow .btn:active,
	.boxshadow .btn-secondary:focus,
	.boxshadow .btn-secondary:active {
		-webkit-box-shadow: 0 0 0 2px $lightGrey2 inset;
		box-shadow: 0 0 0 2px $lightGrey2 inset;
	}

	.boxshadow .content-box {
		box-shadow: none;
	border-radius: 0;
	@include ruler(bottom, $lightGrey2);
	}

	.content-box__row--secondary {
	background-color: transparent;
		border-bottom: 1px solid $darkGrey;
		border-radius: 0;
	}

	.field {
		padding:0 3px;
	}

	#section--remember-me__phone {
		#checkout_vault_phone {
			padding-left: $mobileMargin * 2;
		}
	}

	// Order Summary
	.order-summary-toggle {
		background: $darkGrey;
		padding: $mobileMargin $desktopMargin;
		border: none;
		// @include ruler(bottom,$bodyColor4);

	span {
			@extend .checkoutCopy;
			color: $white;
		}

		.wrap {
			padding: 0;
			background: transparent;
		}

		.order-summary-toggle__icon-wrapper {
			@include icon-fonts-before($icon-bag, $white, 12px);

	  svg {
		display: none;
	  }
		}

		.order-summary-toggle__text {
			@extend .copy10;
			@extend .animated, .flipInX;
			@include animation-duration($slow);

	  color: $lightGrey2;

	  span {
				position: relative;

		@include icon-fonts-after($icon-lod-arrow-right, inherit, 12px);

		&:after {
		  line-height: 1;
		  display: inline-block;
		  margin-left: 5px;
		  font-size: 12px;
		  @include transform(rotate(90deg));
		  @include transition(all $fast);
		}
			}

			svg {
		display: none;
	  }

			&.order-summary-toggle__text--hide {
				span {
			&:after {
				@include transform(rotate(-90deg));
			}
				}
			}
		}
	}//.order-summary-toggle

	// Order Summary Box
	.order-summary {
		@extend .checkoutCopy;

	span {
			@extend .checkoutCopy;
		}

		.product-table {
			margin-bottom:0;
		}

		.product td {
			// padding-top: $mobileMargin;
			padding-bottom: $mobileMargin;
			vertical-align: top;
		}

		//under .order-summary__section--product-list
		.product-thumbnail {
			width: 67px;
			height: auto;
			text-align: center;
			border-radius: inherit;
			background: transparent;

			.product-thumbnail__wrapper {
				border-radius: 0;
			}

			.product-thumbnail__image {
				position: relative;
				max-width: 80px;
				margin:0 auto;
			}

			&::after {
				box-shadow: none;
		border: none;
				border-radius: 0;
			}
		}

		.product-thumbnail__quantity {
			background: $black;
			@include ruler("",$bodyColor4);
			padding:0;
			width: $mobileMargin + 2;
			height: $mobileMargin + 2;
			line-height: $mobileMargin + 2;
			border-radius: 50%;
			font-size: 10px;
			text-align: center;
			@extend .animated, .bounceIn;
			@include animation-delay(1.5s);
			@include animation-duration($fast);
		}

		.product__description {
			padding-left: $mobileMargin;
			padding-right:$mobileMargin;
		}

		.product__description__name,
		.product__description__variant,
		.product__description__property {
			@extend .copy2;
			color: $black;
		}
		.product__price {
			span{
				@extend .copy2;
				color: $black;
			}
		}

		.order-summary__section--product-list:after
		{
			// background-color: $white;
		}

		//under .order-summary__section--discount
		.order-summary__section--discount {
			padding-top: $mobileMargin/2;
			padding-bottom: $desktopMargin2;
			.field__input {
				margin-bottom: 0;
			}
			.btn {
				margin-top: $mobileMargin;
				max-width: 110px;
			}
		}
		.total-line-table {
			td,
			.order-summary__emphasis{
				color: $black;
			}
			td {
				&:before {
					background: $white !important;
				}
			}
		}
		.total-line__price {
			@extend .copy2;

	  span {
				display: inline-block;
				vertical-align: top;
				line-height: 21px;
			}
		}

		.payment-due__currency {
			@extend .copy2;

	  font-size: 10px;
			margin-right: $mobileMargin/2;
		}

		.payment-due__price,
		.payment-due-label__total {
			@extend .copy2;

	  line-height: 21px;
			display: inline-block;
		}
	}//.order-summary

	.map.default-background {
		filter: grayscale(1);
		-webkit-filter: grayscale(1);
		border-radius:0;
	}

	.card-fields-container {
		background: transparent;

	.field__input--iframe-container {
			box-shadow: none !important;
		}

	input {///Forced styles of .checkoutCopy;
			font-family: "ITCFranklinGothicLTsBook" !important;
		font-size: 12px !important;
		line-height: 20px !important;
		font-weight: normal;
		}

		.field__input {
			padding:0;
		}

		.field--show-floating-label .field__input {
			padding:0;
		}
	}

	.popover-backdrop {
		.remember-me {
			.remember-me__code-not-received,
			.remember-me-footer__message{
				background: $white;
			}

			.field--active .remember-me__field-underline-box--active {
				background-color:$black;
			}

	  button {
				span[data-remember-code-not-received-button="regular_checkout"] {
					color: $bodyColor4;
				}
			}
		}
	}//.popover-backdrop

	.modal {
		@extend .copy5;

	color: $black;

	&.default-background {
			border-radius:0;
			background: $white;
			margin-top: 16vh;
		}

		.modal__header {
			padding:$mobileMargin;
			padding-bottom: $mobileMargin/2;

		@include ruler(bottom,$bodyColor2);

		.modal__close {
				button {
					opacity: 1;
					background:none;
					color: $black;
			@include icon-btn($icon-close, inherit, inherit);
				}
			}
		}

		.modal__header__title {
			@extend .copy2;

	  color: $black;
		}

		.modal__content {
			@extend .copy5;

	  color: $black;
			height: 60vh;
		overflow: auto;

	  p {
				font-size: 14px !important;
			}
		}
	}
}//.template-checkout

