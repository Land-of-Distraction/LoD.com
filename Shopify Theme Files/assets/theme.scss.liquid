		.video-ctl {
			width: $video-ctl-size;
			height: $video-ctl-size;
			top: 50%;
			left: 50%;
			@include vcenter;
			@include transform(translate(-50%,-50%) scale(0.9,0.9));
			&:before {
				top: 0;
				left: 0;
				width: 100%;
				height: 100%;
			}
		}
		&:hover {
