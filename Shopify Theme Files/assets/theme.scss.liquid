@include query-tablet-mobile {
	.template-checkout{
		.main{
			padding-top: $desktopMargin;
			h2{
				margin-bottom: $mobileMargin/2;
			}
		}
		.section{
			&.section--reductions{
				height: 0;
				margin-bottom: -4em;
			}
		}
		.section__title{
			margin-bottom: $mobileMargin/2;
		}
		.field__input,
		.boxshadow .main .field__input,
