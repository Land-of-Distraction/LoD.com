//
//  Layout:  PDP, GiftCard PDP
//  Author:  windsor@1R
//
//  Common
//  _____________________________________________

.template-product{
	background:$black;
	#MainContent,
	header.site-header,
	footer.site-footer{
		@include transition(transform $fast ease-in-out);
	}

  &.white-header header.site-header .h1 .site-logo {
	color: #FFF;
  }

  &.white-header header.site-header .header-content nav .level-1 > .nav-link {
	color: #FFF;
  }

}

.product-page{

	.related-products{
		background-color: $black;
		color: $white;
		position: relative;
		text-align: center;
		h4{
			text-transform:uppercase;
			margin-bottom: $mobileMargin*2;
			letter-spacing: 0.5px;
			font-weight: normal;
			font-size: 12px;
		}
		article.item{
			a{
				color: inherit;
			}
			.product-info-wrapper{
				background: transparent;
			}
		}
		.utility-close{
			display: none;
		}
		.grid-system{
			display: flex;
			flex-wrap: wrap;
		}
	}//.related-products

	.utility-close{
		position: absolute;
		right: $mobileMargin;
		top: $mobileMargin;
		cursor: pointer;
		z-index: 1;
		@include icon-btn($icon-close, inherit, 18px);
	}

}//.product-page

.product-single{

	.product-media{
		text-align: center;
		.product-single__photos{
			display: none;
		}
		.images-wrapper{
			a{
				display: block;
				vertical-align: top;
			}
		}
		a{
			display: block;
		}
		img{
			vertical-align: top;
			@extend .animated, .fadeIn;
			@include animation-delay($fast);
			@include animation-duration(0.6s);
		}
		#onezoom{
			@include icon-fonts-before($icon-close, rgba(0,0,0,0.5), 18px);
			&:before{
				position: absolute;
				top: 25px;
				right: 25px;
				cursor: pointer;
				z-index: 1;
			}
		}
		#onezoom-mobile{
			#image-wrap{
				// height: 100vh;
				// width: 100vw;
			}
		}
	}//.product-media

	.product-info{
		color: $white;
		overflow: hidden;
		text-align: center;
		.product-info-wrapper{
			background-color: $black;
			background-position: center;
			background-size: cover;
			background-repeat: no-repeat;
		}
		h1,
		.price-box span{
			@extend .heading2;
			display: block;
			&.product-compare-price{
				text-decoration: line-through;
				opacity: 0.45;
			}
		}
		.selector-wrapper{
			position: relative;
			font-size: 12px;
			@include icon-fonts-after($icon-lod-arrow-right, inherit, 14px);
			.swatch-label{
				color: inherit;
				line-height: $desktopMargin;
				padding-right: $desktopMargin/2;
				text-align: left;
				margin-bottom: 0;
				cursor: pointer;
				overflow: hidden;
				text-overflow: ellipsis;
				@include ruler(bottom);
			}
			&:after{
				position: absolute;
				right: 0;
				top: 4px;
				@include transform(rotate(90deg));
				@include transition(all $fast ease-in-out);
			}
			.swatches{
				position: absolute;
				width: calc(100% + 2px);
				top: $mobileMargin*2;
				left: -1px;
				background: $black;
				padding: 0;
				z-index: 2;
				overflow: hidden;
				visibility: hidden;
				opacity: 0;
				@include transition(all $fast ease-in-out);
				.swatch{
					margin: 0;
					line-height: 1.2;
					text-align: center;
					color: $white;
					display: block;
					font-size: 12px;
					padding: 10px 0;
					white-space: normal;
					@include ruler(bottom,$bodyColor4);
					@include transition(all $fast ease-in-out);
					&:last-child{
						@include ruler(bottom);
					}
					&:hover{
						background: $bodyColor5;
						color: $white;
					}
				}
			}
			&.opened{
				.swatch-label{
				}
				.swatches{
					top: $desktopMargin;
					opacity: 1;
					visibility: visible;
				}
				&:after{
				   @include transform(rotate(-90deg));
				}
			}
		}//.selector-wrapper
		.add-to-cart-form{

		}
		.add-to-cart-wrapper{
			.product-qty-container{
				display: none;
			}
			.add-to-cart-btn{
				span{
					@extend .copy4;
				}
				&:hover{
					background: $white;
					color: $black;
					border-color: $white;
				}
				&.disabled{
					color: $white;
					border-color: $white;
					&:hover{
						background: transparent;
						color: $white;
						border-color: $white;
					}
					& + .product-soldout-text{
						max-height: 50vh;
						opacity: 1;
					}
				}
			}
			.product-soldout-text{
				@extend .copy5;
				color: $white;
				margin-top: $mobileMargin;
				margin-bottom: $mobileMargin;
				max-height: 0;
				overflow: hidden;
				opacity: 0;
				@include transition(all $fast ease-in-out);
				a{
					text-decoration: underline;
				}
			}
		}//.add-to-cart-wrapper
	}//.product-info

	.inner-wrapper{

	}//.inner-wrapper

	.floating-content{
		.product-content-nav{
			text-align: center;
			width: 100%;
			margin: 0 auto;
			display: flex;
			justify-content: space-around;
			a{
				@extend .nav2;
				color: $white;
			}
		}
		.product-content-wrapper{
			.pdp-utility{
				@extend .copy5;
				background: $black;
				padding: $mobileMargin;
				&.open{
					.description-wrapper{
						@include transition-delay($fast);
						@include transform(translateY(0%));
						opacity: 1;
					}
				}
			}
			.description-wrapper{
				@include transition-delay($fast);
				@include transition(opacity $fast ease-in-out);
				@include transform(translateY(0%));
				opacity: 0;
				p{
					margin-top: 0;
					&:last-child{
						margin-bottom: 0;
					}
				}
				em{
					color: $lightGrey2;
					font-style: normal;
				}
			}
		}
	}//.floating-content

	.size-chart{
		@extend .copy5;
		.help-link{
			@extend .nav2;
			position: absolute;
			right: $mobileMargin;
			color: $lightGrey2;
		}
		a{
			@extend .copy5;
			color: $white;
			cursor: pointer;
		}
		.tab-titles{
			display: flex;
			justify-content: flex-start;
			margin-bottom: $mobileMargin;
			a{
				@extend .copy5;
				color: $white;
				@include underline_animation(30%,-5px);
				margin-left: $mobileMargin;
				&:first-child{
					margin-left: 0;
				}
			}
		}
		.tab-container{
			.tabs-content{
				width: 100%;
				position: relative;
				padding: $mobileMargin 0 0;
				text-align: left;
				@include clear-box();
				& > div{
					visibility: hidden;
					opacity: 0;
					float: left;
					width: 100%;
					margin-left: -100%;
					position: relative;
					z-index: 1;
					@include transition(all $fast);
					font-weight: normal;
					line-height: 1.2;
					&.active{
						visibility: visible;
						opacity: 1;
						z-index: 2;
					}
					&:first-of-type{
						margin-left: 0;
					}
				}
			}
		}
		.measures-toggle{
			color: $lightGrey2;
			margin-bottom: $mobileMargin;
			a{
				color: $lightGrey2;
				text-decoration: underline;
				&.active{
					color: $white;
				}
			}
		}
		.chart{
			// display: flex;
			justify-content: flex-start;
			flex-direction: column;

			visibility: hidden;
			opacity: 0;
			position: relative;
			z-index: 1;
			width: 100%;
			table{
				width: 100%;
			}
			.chart-row{
				// display: flex;
				width: 100%;
				// justify-content: space-between;
				@include ruler(top,$bodyColor4);
				td{
					line-height: $desktopMargin;
					font-size: 11px;
					&:first-child{
						color: $bodyColor2;
					}
				}
				&:last-child{
					@include ruler(bottom,$bodyColor4);
				}
			}
			&.with-headers{
				tbody{
					.chart-row{
						&:first-child{
							td{
								color: $bodyColor2;
							}
						}
					}
				}
			}

			&.active{
				visibility: visible;
				opacity: 1;
				z-index: 2;
			}
		}
		.measurement{
			.chart-wrap{
				position: relative;
				@include clear-box();
			}
			.chart{
				float: left;
				width: 100%;
				margin-left: -100%;
				position: relative;
				&.inches{
					margin-left: 0;
				}
			}
		}
	}//.size-chart

	.one-social{
		.share-title{
			display: none;
		}
		.share-list{
			a{
				@extend .copy6;
				margin-right: $mobileMargin;
				text-decoration: none;
				display: inline-block;
				cursor: pointer;
				color: inherit;
				span{
					display: none;
				}
				&:after{
					content: "";
					margin: 0 auto;
					height: 1px;
					display: block;
					background: $black;
					width: 0;
					@include opacity(0);
					@include transition(all $fast);
				}
				&:hover{
					&:after{
						width: 100%;
						@include opacity(1);
					}
				}
			}
		}//.share-list
		.copy-success{
			@extend .animated, .fadeInDown;
			@include animation-delay($fast);
			@include animation-duration(0.6s);
			position: absolute;
			top: 25px;
			left: 0;
			width: 100%;
			text-align: center;
			overflow:hidden;
		}
	}//.one-social

	.scroll-to{
		@extend .nav1;
		cursor: pointer;
		@include icon-fonts-after($icon-lod-arrow-right, inherit, inherit);
		&:after{
			display: none;
			margin-left: 8px;
			vertical-align: top;
			margin-right: -8px;
			@include transition(all $fast ease-in-out);
		}
		&:hover{
			// letter-spacing: 0.5px;
			&:after{
				margin-left: 12px;
			}
		}
	}//.scroll-to

	&.sold-out{
		.product-info{
			.selector-wrapper{
				.swatches{
					.swatch{
						color: $lightGrey2;
					}
				}
			}
		}
	}//&.sold-out

	&.gift-card{
		.product-info{
			.selector-wrapper{
				width: 100%;
			}
		}
		.scroll-to,
		a[href="#size-chart"]
		{
			display: none;
		}
	}

}//.product-single

