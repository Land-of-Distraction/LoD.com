		@extend .copy5;
		line-height: 21px;
		.page-title{
			@extend .copy7;
			margin:0;
			padding:0;
			height: 0;
			max-height: 0;
			overflow:hidden;
			width: 0;
		}
		h2,h3,h4,h5,h6{
			// margin-top: $mobileMargin;
			margin-bottom: $mobileMargin;
			@extend .copy2;
		}
		p{

		}
		ul{
			li{
				position: relative;
				padding-left: $desktopMargin/2;
				&:before{
					content:"";
					position: absolute;
					top: 9px;
					left: 0;
					width: 2px;
					height: 2px;
					border-radius:50%;
					background:white;
				}
			}
		}
	}//.page-content

	&.page-utility{
		@extend .copy5;
	}

	&.about-page{
		overflow: hidden;
		.page-title{
			@extend .copy7;
			margin:0;
			padding:0;
