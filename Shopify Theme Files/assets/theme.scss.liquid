				position: absolute;
				top: 0;
				bottom: 10px;
				left: 0;
				width: 100%;
				z-index: 3;
				text-indent: -99999px;
				// opacity: 0;///hide controls
				&:before{
					position: absolute;
					left: 50%;
					top: 72%;
					@include transform(translate(-50%,-50%));
					width: $mobileMargin*2;
					height: $mobileMargin*2;
					background: transparent;
					border:2px solid $white;
					border-radius: 50%;
					text-align: center;
					line-height: $mobileMargin*2 - 6;
