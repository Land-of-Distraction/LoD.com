	$desktop: 1025px;
	$smallDesktop: 1200px;
	$pre-desktop: $desktop - $viewport-increment;
	// Tablet
	$tablet: 1024px;
	$screen__break: $tablet;
	$pre-tablet: $tablet - $viewport-increment;
	// Mobile
	$mobile: 768px;
	$pre-mobile: $mobile - $viewport-increment;
	$post-mobile: $mobile + $viewport-increment;
	// Constraints
	$min: min-width;
	$max: max-width;

/* Colors */
	$black: #000000;
	$white: #ffffff;
	$lightRed: #FE6262;
	$lightGreen: #59a263;
