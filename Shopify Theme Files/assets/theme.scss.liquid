.template-collection {

	padding-top: $headerMobileHeight + $promoBannerHeight;
	// @include transform(translateX(0));
	@include transition(all $fast ease-in-out);

	.in-grid-promo{
		position: relative;
	}
}

.collection-header {
	height: 0;
	overflow: hidden;
	max-height: 0;
	opacity: 0;
}

.collection-nav {
	width: 100%;
	position: relative;
	@include clear-box();

	.collection-nav-toggle{
		@extend .nav1;
		position: relative;
		margin-bottom: $mobileMargin;
		display: block;
		// display: none;//temporary
		cursor: pointer;
		@include transition(all $fast ease-in-out);
		@include icon-fonts-before($icon-lod-arrow-right, inherit, inherit);
		&:before{
			display: inline-block;
			margin-right: 8px;
			vertical-align: top;
			margin-left: -8px;
			@include transition(all $fast ease-in-out);
		}
		&:hover{
			// letter-spacing: 0.5px;
			&:before{
				margin-right: 15px;
			}
		}
	}

	.collection-nav-wrapper{
		background: $black;
		color: $white;
		padding: $mobileMargin;
		position: fixed;
		top: 0;
		bottom: 0;
		left: 0;
		overflow: auto;
		// @include transform(translateX(-100%));
		@include transition(all $fast ease-in-out);
		@include clear-box();
	}

	.collection-nav-close{
		position: absolute;
		right: $mobileMargin;
		top: $mobileMargin;
		cursor: pointer;
		@include icon-btn($icon-close, inherit, 18px);
	}

	.collection-contain{
	}

	.filter-select { display: none; }
	.collection-nav-block {
		@extend .nav2;
		.nav-title {
			@extend .nav1;
			cursor: pointer;
			margin-bottom: $mobileMargin*2;
		}
		.nav-content {
			width: 100%;
			z-index: 1;
			// margin-bottom: $mobileMargin*2;
			&, a, span {
				color: inherit;
			}

			.nav-list-container {
				display: flex;
				flex-wrap: wrap;
				justify-content: flex-start;
			}
			.filter {
				width: 100%;
				display: block;
				.filter-title{
					@extend .nav1;
					display: inline-block;
					margin-bottom: $mobileMargin;
				}
				.filter-swatches{
					margin-bottom: $desktopMargin;
					display: block;
					.filter-swatch{
						width: calc(98%/3);
						display: inline-block;
						vertical-align: top;
						margin-bottom: $desktopMargin/2;
						text-transform: capitalize;

						&:not(.clear-all):before{
							content: "";
							width: 10px;
							height: 10px;
							background: transparent;
							border: 1px solid $white;
							display: inline-block;
							margin-right: $mobileMargin/2;
							@include transition(all $fast ease-in-out);
						}
						// &:hover{
						//     &:before{
						//         margin-right: $desktopMargin/2;
						//     }
						// }
						&.disabled {
							opacity: 0.5;
							cursor: default;
						}
						&.active {
							&:before{
								width: 0;
								height: 0;
								border-width:5px;
							}
						}
					}
				}//.filter-swatches
				&.clear-all {
					@include icon-fonts-before($icon-close, inherit, 10px);
					margin-bottom: 0;
					&:before{
						@include transition(all $fast ease-in-out);
						margin-right: $mobileMargin/2;
					}
				}
			}
		}//.nav-content
		&.filters{
			.nav-title{
				display: none;
			}
		}
		&.sorting{
			.hide{
				display: none;
			}
			.nav-content{
				.filter{
					width: 100%;
					margin-bottom: $mobileMargin;
					text-transform: capitalize;
					&:not(.clear-all):before{
						content: "";
						width: 10px;
						height: 10px;
						background: transparent;
						border: 1px solid $white;
						border-radius:50%;
						display: inline-block;
						margin-right: $mobileMargin/2;
						@include transition(all $fast ease-in-out);
					}
					&.disabled {
						opacity: 0.5;
					}
					&.active {
						&:before{
							width: 0;
							height: 0;
							border-width:5px;
						}
					}
				}
				// &:hover{
				//     &:before{
				//         margin-right: $desktopMargin/2;
				//     }
				// }
			}
		}
	}//.collection-nav-block

}//.collection-nav

.collection-nav-opened {

}

article.item {
	text-align: center;
	position:relative;
	width: 50%;
	@include clear-box();
	@include transition(all $slow ease-in-out);
	padding-bottom: $mobileMargin;
	overflow: hidden;

	a {
		display: block;
		position: relative;
	}

	.product-image-wrapper {
		position: relative;
		overflow: hidden;
		// @include vcenter();
		display: flex;
		justify-content: center;
		align-items: center;
		z-index: 1;

		picture {
			display: block;
		}

		img {
			width: 100%;
			vertical-align: top;

			@include transition(opacity $fast ease-in-out);
		}

		.hidden > img {
			position: absolute;
			z-index: 0;
			left: 0;
			top: 0;
			width: 100%;
			@include opacity(0);
		}
	}

	.quick-buy-container,
	.sold-out {
		position: absolute;
		width: 100%;
		padding: $mobileMargin;
		bottom: 0;
		left:0;
		background: $black;
		color: $white;
		visibility: hidden;
		opacity: 0;
		@include transform(translateY(100%));
		@include transition(all $quick ease-in);
	}

	.quick-buy-container {
		.add-to-text {
			@extend .nav2;
			display: block;
			margin-bottom: $desktopMargin/2;
		}

		.selector-wrapper .swatch-label,
		.cta-wrapper {
			display: none;
		}

		.selector-wrapper {
			margin-bottom: 0;

			.swatches {
				&.colors {
					display: none;
				}

				.swatch {
					color: $white;
				}
			}
		}

		&.active {
			visibility: visible;
			opacity: 1;
			@include transform(translateY(0%));
		}
	}

	.sold-out{
		@extend .copy3;
		padding: $mobileMargin*2 0;
	}
	.product-info-wrapper{
		padding-top: $mobileMargin;
		text-align: center;
		background: $white;
		position: relative;
		z-index: 2;
		.product-name{
			@extend .copy2;
		}
		.price-box{
			& * {
				@extend .copy2;
			}
			.product-price{

			}
			.product-compare-price{
				display: inline;
				text-decoration: line-through;
				color: $lightGrey2;

			}
		}

		.quick-buy-ctl{
			display: none;
		}
	}

}//article.item

html.no-touchevents {
	article.item {
		&:hover {
			.product-image-wrapper {
				img {
					@include opacity(0);
				}

				.hidden > img {
					@include opacity(1);
				}

				&.single-image {
					img {
						@include opacity(1);
					}

					.hidden > img {
						@include opacity(0);
					}
				}
			}

			.quick-buy-container,
			.sold-out {
				visibility: visible;
				opacity: 1;
				@include transform(translateY(0%));
			}
		}
	}
}//html.no-touchevents

.product-collection {
	padding: $mobileMargin;
	overflow-x: hidden;
}

.collection-products {/// product grid
	@include clear-box();
	display: flex;
	flex-wrap: wrap;
	padding: 0;

	article.item{
		width: calc((100% - #{$mobileMargin}) / 2);
		@include transform(translateY(0%));
		&.promo-item{
			vertical-align: top;
			img{
				max-width: 320px;
				width: 100%;
			}
		}
	}
	&.sortlowest{
		article.item{
			&:nth-child(-n+6).promo-item{
				display: none;
				 + .promo-item{
					display: none;
				}
			}
		}
	}
}

.collection-load-more {
	display: block;
	text-align: center;
	line-height: 40px;
	opacity: 0;
	visibility: visible;
	background: $white;
	@include transition(all $slow);
	&.disabled {
		line-height: 0;
		opacity: 0;
		visibility: hidden;
	}
}

.collection-pagination {
	display: none;
}

