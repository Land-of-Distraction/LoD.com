@include desktop-only {
	$spacingTop : $headerDesktopHeight + $promoBannerHeight + $desktopMargin*2;

	$collectionNavWidth: 34%;
	$negCollectionNavWidth: -#{$collectionNavWidth};

	.template-collection{
		padding-top: $spacingTop;
		#MainContent,
		header.site-header,
		footer.site-footer{
			width: 100vw;
			@include transition(margin-left $fast ease-in-out);
		}
	}

	.product-collection{
		padding: 0 $desktopMargin*2 $desktopMargin*2;
		min-height: 100vh;
	}

	.collection-nav {
		.collection-nav-toggle{
			margin-bottom: $desktopMargin;
		}
		.collection-nav-wrapper{
			width: $collectionNavWidth;
			left: $negCollectionNavWidth;
			z-index: 100;
			padding: $mobileMargin*2;
			padding-top: $spacingTop;
		}
		.collection-nav-block{
			opacity: 0;
			@include transition(opacity $fast ease-in-out);
			@include transition-delay($fast);
			.nav-content{
				.filter{
					&.filter-size{
						width: 75%;
						.filter-swatches{
							.filter-swatch{
								width: calc(88%/2);
							}
						}
					}
				}
			}
		}
	}//.collection-nav

	.collection-nav-opened{//on body
		overflow-x: hidden;
		//1. Move content elements to the right
		#MainContent,
		header.site-header,
		footer.site-footer{
		   margin-left: $collectionNavWidth;
		}

		//2. Keep the nav on the left edge while the other content is on the side
		.collection-nav{
			.collection-nav-wrapper{
				left: 0;
			}
			.collection-nav-block{
				opacity: 1;
			}
		}
	}

	.collection-products {
		padding: 0;
		margin:0 -#{$desktopMargin};

		article.item{
			opacity: 1;
			padding-bottom: $desktopMargin;
			margin:0 $desktopMargin $desktopMargin;
			width: calc((100% - #{$desktopMargin*6}) / 3);//3 columns and twice the gutter
			a{
				width: 100%;
			}
			&.promo-item{
				padding-bottom: $desktopMargin*2;
			}
		}
		.gutter-sizer{
			width: $desktopMargin*2;
		}

		&.grid-adjusted{
			article.item{
				opacity: 1;
				@include transition-delay($fast);
				@include transform(translateY(0%));
			}
		}
	}//.collection-products
};//desktop-only

