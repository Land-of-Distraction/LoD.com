@mixin icon-fonts-base(){
	font-family: #{$iconFont} !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
@mixin icon-fonts($content, $color, $size){
	@include icon-fonts-base();
	@if $color != '' { color: $color; }
	content: $content;
	font-size: $size;
	text-indent: 0;
}
@mixin icon-fonts-before($content, $color, $size){
	// text-indent: 100%;
	// white-space: nowrap;
	&:before {
		@include icon-fonts($content, $color, $size);
	}
}
@mixin icon-fonts-after($content, $color, $size){
	// text-indent: 100%;
	white-space: nowrap;
	&:after {
		@include icon-fonts($content, $color, $size);
	}
}
@mixin icon-btn($content, $color, $size) {
	@include icon-fonts-before($content, $black, $size);
	border: 0;
	background: none;
	padding: 0;
	margin: 0;
	span { display: none;}
}

[class^="icon-"], [class*=" icon-"] {
	@include icon-fonts-base();
}

// CTAs

.cta.add-to-cart-btn {
	color: white;
	border-radius: 0;
	border: 1px solid white;
}

.cta.checkout-btn {
	color: white;
	border-radius: 0;
	border: 1px solid white;
}

$cta-height: 32px;
$cta-height-mobile: 36px;
@mixin cta {
	@extend .copy4;
	width: 100%;
	background: transparent;
	color: black;
	border-radius: 0;
	border: 1px solid black;
	display: block;
	line-height: $cta-height;
	text-align: center;
	cursor: pointer;
	@include transition(all $slow);
	&:hover {
		background-color: $white;
		color: $black;
	}
	@media (max-width: $screen__break){
		line-height: $cta-height-mobile;
	}
}
.cta {
	@include cta;
}
@mixin cta-light {
	@include cta;
	background: $white;
	color: $black;
	@include ruler('',$black);
}
.cta-light {
	@include cta-light;
}
