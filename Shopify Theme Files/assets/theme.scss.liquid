				text-align: center;
				display: flex;
				align-items: center;
				.images-blocks{
					display: flex;
					flex-direction: column-reverse;
					align-items:center;
					.image-block{
						&:first-child{
							width: 65%;
							margin: 0 auto;
						}
						&:nth-child(2){
							margin-bottom: $desktopMargin;
						}
					}
				}
				.block-cta{
					text-align: left;
				}
			}

			&.layout-2{
				padding: $mobileMargin*2 12%;
				.images-blocks{
					.image-block{
						display: inline-block;
						margin-bottom: $desktopMargin;
						&:last-child{
							margin-bottom: 0;
						}
					}
				}
				.block-cta{
					text-align: right;
				}
			}

			&.layout-3{
				padding: 8vh 12%;
				.images-blocks{
					.image-block{
						img{
							vertical-align: top;
						}
						&:first-child{
							height: 42vh;
							text-align: center;
							vertical-align: top;
							img{
								width: auto;
								height: 42vh;
								margin: 0 auto;
							}
						}
						&:nth-child(2){
							width: 55%;
							position: absolute;
							left: 0;
							top: 50vh;
							.block-cta{
								display: none;
							}
						}
						&:last-child{
							width: 80%;
							position: relative;
							margin-top:20vh;
							float: right;
						}
					}
				}
				.block-cta{
					text-align: right;
